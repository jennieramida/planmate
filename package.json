{
  "name": "planmate",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Jennie <jennieramida@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build-firebase": "rm -rf nuxt && rm -rf functions/nuxt && nuxt build && cp -R nuxt functions/ && cp -R static/* nuxt/dist/ && npm run deploy",
    "start": "PORT=80 nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy": "cd functions && npm install && firebase deploy --only functions,hosting"
  },
  "dependencies": {
